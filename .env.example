# Service account JSON (either the JSON string or a path to a JSON file)
GOOGLE_SERVICE_ACCOUNT_JSON=

# Optional overrides
VERTEX_PROJECT_ID=
VERTEX_LOCATION=global
NANO_BANANA_MODEL=gemini-3-pro-image-preview
NANO_BANANA_GCS_BUCKET=
NANO_BANANA_GCS_PREFIX=nano-banana/refs
NANO_BANANA_OUTPUT_GCS_BUCKET=
NANO_BANANA_OUTPUT_GCS_PREFIX=nano-banana/outputs
NANO_BANANA_OUTPUT_DIR=~/nano-banana-outputs

# Optional IMG.LY background removal settings
NANO_BANANA_IMGLY_MODEL=medium
NANO_BANANA_IMGLY_OUTPUT_FORMAT=image/png
NANO_BANANA_IMGLY_OUTPUT_QUALITY=
NANO_BANANA_IMGLY_PUBLIC_PATH=

# Auto-transparency keying (used when prompt requests transparency)
NANO_BANANA_TRANSPARENCY_KEY_COLOR=#00ff00
# Optional override (leave blank for auto)
NANO_BANANA_TRANSPARENCY_TOLERANCE=
NANO_BANANA_TRANSPARENCY_FEATHER=6
