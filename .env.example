# Service account JSON (either the JSON string or a path to a JSON file)
GOOGLE_SERVICE_ACCOUNT_JSON=

# Optional overrides
VERTEX_PROJECT_ID=
VERTEX_LOCATION=global
NANO_BANANA_MODEL=gemini-3-pro-image-preview
NANO_BANANA_GCS_BUCKET=
NANO_BANANA_GCS_PREFIX=nano-banana/refs
NANO_BANANA_OUTPUT_GCS_BUCKET=
NANO_BANANA_OUTPUT_GCS_PREFIX=nano-banana/outputs
NANO_BANANA_OUTPUT_DIR=~/nano-banana-outputs
# Optional progress heartbeat interval (ms) to keep long MCP calls alive (0 disables)
NANO_BANANA_PROGRESS_INTERVAL_MS=20000
# Auto-task 4K generations to avoid client timeouts (set true to enable)
NANO_BANANA_AUTO_TASK_4K=false
# Auto-task TTL in ms (0 disables expiry)
NANO_BANANA_AUTO_TASK_TTL_MS=1200000
